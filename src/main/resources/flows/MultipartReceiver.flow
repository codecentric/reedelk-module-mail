{
  "id": "72ee9c4b-17f3-48c2-b1bd-1fe1a9781a5d",
  "title": "Multipart Receiver",
  "description": "Multipart Receiver Flow",
  "flow": [
    {
      "implementor": "com.reedelk.rest.component.RestListener",
      "description": "REST Listener",
      "configuration": {"ref": "dc618f88-ab97-4954-a377-7d6b2670627e"},
      "path": "/multipart",
      "method": "POST",
      "streaming": "AUTO",
      "response": {
        "body": "#[message.payload()]",
        "status": 200
      },
      "errorResponse": {
        "body": "#[error]",
        "status": 500
      }
    },
    {
      "implementor": "com.reedelk.core.component.LoggerComponent",
      "description": "Logger",
      "level": "INFO",
      "message": "#[message.payload().myfile]"
    }
  ]
}